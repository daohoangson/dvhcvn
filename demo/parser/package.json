{
  "name": "parser",
  "scripts": {
    "build": "rollup --config",
    "deno:run": "deno run --allow-read bin/cli.ts",
    "format": "prettier --write '**/*.{js,json,ts,tsx}'",
    "lint": "tsc && eslint '*/**/*.{ts,tsx}' --quiet --fix",
