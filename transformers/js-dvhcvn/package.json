  "bugs": {
    "url": "https://github.com/daohoangson/dvhcvn/issues"
  },
  "homepage": "https://github.com/daohoangson/dvhcvn#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.2",
    "jest": "^27.0.6",
    "rollup": "^2.56.2",
    "rollup-plugin-ts": "^1.4.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-standard": "^10.0.0",
