{
  "name": "dvhcvn",
  "version": "1.1.20240201",
  "description": "Three tiers of Vietnam's administrative units (last update: 01/02/2024)",
  "files": [
    "lib/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "prebuild": "npm run generate",
    "format": "prettier --write '**/*.{js,json,ts}'",
    "generate": "tsx bin/generate.ts ../../data/dvhcvn.json > src/data.ts",
    "postgenerate": "npm run format && npm run test:coverage",
    "test": "vitest",
    "test:coverage": "vitest --coverage --no-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daohoangson/dvhcvn.git"
  },
  "keywords": [
    "vietnam",
    "provinces",
    "cities",
    "districts"
  ],
  "author": "Dao Hoang Son <daohoangson@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/daohoangson/dvhcvn/issues"
  },
  "homepage": "https://github.com/daohoangson/dvhcvn#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^16.6.2",
    "@vitest/coverage-v8": "^0.34.5",
    "prettier": "^2.8.8",
    "rollup": "^3.29.3",
    "tslib": "^2.6.2",
    "tsx": "^3.14.0",
    "typescript": "^5.3.3",
    "vitest": "^0.34.5"
  }
}
